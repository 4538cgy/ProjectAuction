<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <data>
        <variable
            name="itemlayoutsetting"
            type="com.example.project_auction.data.SettingItem" />
    </data>

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginLeft="24dp"
        android:layout_marginTop="8dp"
        >
        <ImageView
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"
            android:id="@+id/item_layout_setting_imageview"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            />

        <TextView
            android:id="@+id/item_layout_setting_textview"
            app:layout_constraintTop_toTopOf="@+id/item_layout_setting_imageview"
            app:layout_constraintBottom_toBottomOf="@+id/item_layout_setting_imageview"
            app:layout_constraintLeft_toRightOf="@+id/item_layout_setting_imageview"
            android:layout_marginLeft="12dp"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="item"
            />

    </androidx.constraintlayout.widget.ConstraintLayout>

</layout>